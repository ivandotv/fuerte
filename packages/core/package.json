{
  "name": "@fuerte/core",
  "version": "1.1.0",
  "private": false,
  "author": "Ivan Vlatkovic",
  "description": "Backbone.js inspired Mobx library",
  "buildTarget": "server",
  "main": "dist/cjs/index.js",
  "module": "dist/esm/core.esm.js",
  "unpkg": "dist/unpkg/core.min.js",
  "jsdelivr": "dist/unpkg/core.min.js",
  "exports": {
    "import": "./dist/esm/core.esm.js",
    "require": "./dist/cjs/index.js",
    "development": "./dist/cjs/core.development.js"
  },
  "sideEffects": false,
  "types": "dist/types/",
  "files": [
    "dist",
    "src"
  ],
  "publishConfig": {
    "access": "public"
  },
  "license": "MIT",
  "scripts": {
    "lint": "eslint --fix \"src/**/\"",
    "lint:ci": "eslint \"src/**/\"",
    "docs": "del docs/api && typedoc --theme markdown --readme none --excludePrivate  --excludeInternal --exclude ./src/globals.d.ts --out docs/api ./src",
    "test": "cross-env BABEL_ENV=test jest --verbose --coverage --no-cache",
    "test:watch": "cross-env BABEL_ENV=test jest --watch",
    "test:ci": "cross-env BABEL_ENV=test jest --runInBand --ci --coverage --coverageDirectory=coverage",
    "type:check": "del .buildcache/core/check-types.tsbuildinifo && tsc -p tsconfig.check-types.json",
    "type:build": "del dist/types && del .buildcache && tsc -p tsconfig.build-types.json",
    "build": "yarn type:check && yarn type:build && node ../../scripts/build-browser.js --bundle=umd,cjs,esm",
    "prepublishOnly": "yarn build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ivandotv/fuerte.git"
  },
  "bugs": {
    "url": "https://github.com/ivandotv/fuerte/issues"
  },
  "homepage": "https://github.com/ivandotv/fuerte#readme",
  "keywords": [
    "mobx",
    "@fuerte"
  ],
  "dependencies": {
    "fast-deep-equal": "^3.1.3",
    "nanoid": "^3.1.25"
  },
  "devDependencies": {
    "@types/jest": "^27.0.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-jest": "^24.4.0",
    "eslint-plugin-prettier": "^4.0.0",
    "mobx": "^6.3.3"
  },
  "peerDependencies": {
    "mobx": "^6.3.3"
  }
}
