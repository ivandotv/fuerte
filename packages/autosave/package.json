{
  "name": "@fuerte/autosave",
  "version": "0.0.0",
  "private": true,
  "author": "Ivan Vlatkovic",
  "description": "Autosave collection for @fuerte/core",
  "main": "dist/cjs/index.js",
  "module": "dist/esm/autosave.esm.js",
  "unpkg": "dist/unpkg/autosave.min.js",
  "jsdelivr": "dist/unpkg/autosave.min.js",
  "exports": {
    "import": "./dist/esm/autosave.esm.js",
    "require": "./dist/cjs/autosave.js",
    "development": "./dist/cjs/autosave.development.js"
  },
  "sideEffects": false,
  "types": "dist/types",
  "files": [
    "dist",
    "src"
  ],
  "scripts": {
    "lint": "eslint --fix \"src/**/\"",
    "lint:ci": "eslint \"src/**/\"",
    "test": "cross-env BABEL_ENV=test jest --verbose --coverage --no-cache",
    "test:watch": "cross-env BABEL_ENV=test jest --watch",
    "test:ci": "cross-env BABEL_ENV=test jest --runInBand --ci --coverage --coverageDirectory=coverage",
    "type:check": "tsc -p tsconfig.check-types.json",
    "type:build": "del dist/types && del .buildcache && tsc -p tsconfig.build-types.json",
    "build": "yarn type:build && node ../../scripts/build-browser.js --bundle=umd,cjs,esm",
    "prepublishOnly": "yarn build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ivandotv/fuerte.git"
  },
  "bugs": {
    "url": "https://github.com/ivandotv/fuerte/issues"
  },
  "homepage": "https://github.com/ivandotv/fuerte#readme",
  "keywords": [
    "mobx",
    "@fuerte"
  ],
  "devDependencies": {
    "@types/jest": "^27.0.1",
    "@fuerte/core": "*",
    "mobx": "^6.3.3"
  },
  "peerDependencies": {
    "@fuerte/core": "*",
    "mobx": "^6.3.3"
  }
}
